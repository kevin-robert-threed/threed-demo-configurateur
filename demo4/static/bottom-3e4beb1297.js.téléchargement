function formatContent(opt){return $('<div class="select-pack-item-option d-flex" data-cover="'+$(opt.element).data('cover')+'">'+'<div class="select-pack-item-name d-flex">'+'<div class="ref">'+$(opt.element).data('ref')+'</div>'+'<div class="attribute">'+$(opt.element).data('attribute')+'</div>'+'</div>'+'<div class="price">'+$(opt.element).data('price-impact')+'</div>'+'</div>')};function updateView(DOMobject){let select=$(DOMobject.target);let data=select.select2('data');let option=select.find("option[value='"+data.id+"']");updateImg(DOMobject,select,data,option);updateProductPrice(DOMobject)}
function updateImg(DOMobject,select,data,option){let container=$(DOMobject.target).parents('.productPacksRow');let cover=option.data('cover');if(cover){container.find('img').attr('src',cover)}}
function updateProductPrice(DOMobject){let container=$(DOMobject.target).parents('.productPacks');let priceImpact=0;let priceWithoutPack=0;let priceWithoutReduction=0;if($('.product-price--old-discount').length){priceWithoutReduction=$('.product-price--old-discount').data('price-without-reduction')}
container.find('.productPacksRow').each(function(i,e){select=$(e).find('.packItemSelect');if(select.length){let data=select.select2('data');let option=select.find("option[value='"+data.id+"']");if($(option).length){let price=$(option).data('price');if(isNaN(price))
