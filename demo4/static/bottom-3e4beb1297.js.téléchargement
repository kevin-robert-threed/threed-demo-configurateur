currentPriceWithoutPack=0;priceImpact+=parseFloat(price*$(option).data('quantity'));priceWithoutPack+=parseFloat(currentPriceWithoutPack*$(option).data('quantity'))}}else{if(!priceWithoutReduction){let input=$(e).find('.stdProduct');priceWithoutPack+=parseFloat($(input).data('price')*$(input).data('quantity'))}}});if(priceWithoutReduction>0){priceWithoutPack+=priceWithoutReduction+priceImpact}
let price=0;let ratio=0;if($('.product-price--old-discount').length){price=$('.product-price--old-discount').data('price-without-reduction')+priceImpact}else{price=parseFloat(container.data('originPrice'))+priceImpact}
if(priceWithoutReduction>0){let type=$('.product-price--old-discount').data('reduction-type');let reduction=$('.product-price--old-discount').data('reduction');if(type=='percentage'){price=(1-reduction)*price;ratio=Math.ceil(reduction*100)}else{price-=reduction;ratio=Math.ceil(parseFloat(reduction/price*100))}}
if(ratio===0){ratio=Math.ceil((1-(price/priceWithoutPack))*100)}
price=formatPrice(price);priceWithoutPack=formatPrice(priceWithoutPack);$('#pack_price').html(price);$('#price_without_pack').html(priceWithoutPack);$('#totalPackreduction').html(""+ratio)}
function formatPrice(price){if(Number.isInteger(price)){price+='<sup>â¬00&nbsp;HT</sup>'}else{price=price.toFixed(2)+'';var priceSplitted=price.split('.');price=priceSplitted[0]+'<sup>â¬'+priceSplitted[1]+'&nbsp;HT</sup>'}
return price};/**
